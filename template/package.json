{
  "name": "{{ name }}",
  "version": "1.0.6",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    "dev": "set BABEL_ENV=development && node build/dev-server.js",
    "dev:test": "export BABEL_ENV=testing && export DEV_ENV=testing && node build/dev-server.js",
    "dev:lab": "export BABEL_ENV=labing && export DEV_ENV=labing && node build/dev-server.js",
    "dev:pro": "export BABEL_ENV=production && export DEV_ENV=production && node build/dev-server.js",
    "start": "export BABEL_ENV=development && node build/dev-server.js",
    "build": "export BABEL_ENV=production && export DEV_ENV=production && node build/build.js",
    "build:lab": "export BABEL_ENV=production && export DEV_ENV=labing && node build/build.js",
    "build:test": "export BABEL_ENV=testing && export DEV_ENV=testing && node build/build.js"{{#unit}},
    "unit": "cross-env BABEL_ENV=testing karma start test/unit/karma.conf.js --single-run"{{/unit}}{{#e2e}},
    "e2e": "node test/e2e/runner.js"{{/e2e}}{{#if_or unit e2e}},
    "test": "{{#unit}}npm run unit{{/unit}}{{#unit}}{{#e2e}} && {{/e2e}}{{/unit}}{{#e2e}}npm run e2e{{/e2e}}"{{/if_or}}{{#lint}},
    "lint": "eslint --ext .js,.vue src{{#unit}} test/unit/specs{{/unit}}{{#e2e}} test/e2e/specs{{/e2e}}"{{/lint}}
  },
  "dependencies": {
    {{#ui}}
    {{#if_eq uiConfig "em-fe"}}
    "em-fe": "^1.0.29",
    "iscroll": "^5.2.0",
    {{/if_eq}}
    {{/ui}}
    {{#jsonp}}
    "em-jsonp": "^1.0.4",
    {{/jsonp}}
    "em-normalize": "^1.0.2",
    "em-underline": "^1.0.9",
    "em-cookie": "^1.0.6",
    "nprogress": "^0.2.0",
    "vue": "{{ vueversion }}",
    "vuex": "^2.3.1"{{#router}},
    "vue-router": "^2.3.1"{{/router}}
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "axios": "^0.16.2",
    {{#stylelint}}
    "stylelint-webpack-plugin": "^0.7.0",
    {{/stylelint}}
    {{#lint}}
    "babel-eslint": "^7.1.1",
    {{/lint}}
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^1.1.3",
    "compression-webpack-plugin": "^1.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    {{#lint}}
    "eslint": "^3.19.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.0",
    {{#if_eq lintConfig "standard"}}
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    {{/if_eq}}
    {{#if_eq lintConfig "airbnb"}}
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    {{/if_eq}}
    {{/lint}}
    {{#ui}}
    {{#if_eq uiConfig "iview"}}
    "iview": "^2.0.0-rc.17",
    {{/if_eq}}
    {{#if_eq uiConfig "element"}}
    "element-ui": "^1.3.7",
    {{/if_eq}}
    {{/ui}}
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "webpack-bundle-analyzer": "^2.2.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    {{#unit}}
    "cross-env": "^4.0.0",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
