    "karma-webpack": "^2.0.2",
    "lolex": "^1.5.2",
    "mocha": "^3.2.0",
    "chai": "^3.5.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "inject-loader": "^3.0.0",
    "babel-plugin-istanbul": "^4.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    {{/unit}}
    {{#e2e}}
    "chromedriver": "^2.27.2",
    "cross-spawn": "^5.0.1",
    "nightwatch": "^0.9.12",
    "selenium-server": "^3.0.1",
    {{/e2e}}
    "semver": "^5.3.0",
    "shelljs": "^0.7.8",
    "opn": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "ora": "^1.2.0",
    "postcss": "^6.0.1",
    "postcss-initial": "^2.0.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-nested": "^2.0.2",
    "postcss-nested-ancestors": "^1.0.0",
    "postcss-nested-props": "^1.1.7",
    "rimraf": "^2.6.0",
    "url-loader": "^0.5.8",
    "vue-loader": "^12.1.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "{{ vueversion }}",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.18.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.5",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "iOS > 7",
    "Android >= 4.0",
    "last 2 versions",
    "not ie <= 8"
  ]
}
